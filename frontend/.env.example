# React Environment Configuration Example
# Copy this file to .env and update values for your environment

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_BASE_URL=http://localhost:3000

# Development settings
GENERATE_SOURCEMAP=true
FAST_REFRESH=true

# Build settings
BUILD_PATH=build
PUBLIC_URL=/

# Optional: Disable browser opening on start (set to 'none' to disable)
BROWSER=chrome